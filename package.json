{
  "name": "Voke",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:jetifier": "npx jetifier",
    "ios": "react-native run-ios",
    "ios:iphone8": "react-native run-ios --simulator='iPhone 8'",
    "ios:ipad": "react-native run-ios --simulator='iPad'",
    "ios:release": "react-native run-ios --configuration=release",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "jest --coverage=true",
    "detox:build:ios": "detox build --configuration ios",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "detox:test:ios": "clear && detox test --configuration ios",
    "detox:test:reuse": "clear && detox test  --reuse --configuration ios",
    "detox:test:singlefile": "clear && detox test --configuration ios -f /e2e/adventureGroupDaily.e2e.js --debug-synchronization 400000",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier ./src --ext .js,.jsx,.ts,.tsx",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run check-format && npm run lint",
    "onesky:upload": "babel-node ./bin/oneskyUpload.js",
    "onesky:download": "babel-node ./bin/oneskyDownload.js",
    "postinstall": "react-native-inhibit-warnings",
    "pods": "npx pod-install",
    "clean": "npx react-native-clean-project"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "^3.0.8",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/push-notification-ios": "^1.8.0",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-firebase/analytics": "^10.3.1",
    "@react-native-firebase/app": "^10.3.0",
    "@react-native-firebase/crashlytics": "^10.3.1",
    "@react-native-firebase/dynamic-links": "^10.3.1",
    "@react-native-firebase/perf": "^10.3.1",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@welldone-software/why-did-you-render": "^6.0.3",
    "formik": "^2.2.6",
    "i18next": "^19.8.4",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "normalizr": "^3.6.1",
    "qs": "^6.9.4",
    "react": "17.0.1",
    "react-i18next": "^11.8.4",
    "react-native": "0.64.0-rc.2",
    "react-native-bootsplash": "^3.1.2",
    "react-native-communications": "^2.2.1",
    "react-native-device-info": "^7.3.1",
    "react-native-fast-image": "^8.3.4",
    "react-native-fbsdk": "3.0.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-image-crop-picker": "^0.35.2",
    "react-native-image-picker": "^3.1.2",
    "react-native-localize": "^2.0.1",
    "react-native-material-menu": "^1.2.0",
    "react-native-modal-datetime-picker": "^9.1.0",
    "react-native-modalize": "^2.0.8",
    "react-native-orientation-locker": "git+https://github.com/wonday/react-native-orientation-locker.git#master",
    "react-native-permissions": "^3.0.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-portalize": "^1.0.7",
    "react-native-push-notification": "^6.1.3",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-scalable-image": "^1.0.0",
    "react-native-screens": "^2.16.1",
    "react-native-share": "^4.1.1",
    "react-native-skeleton-content-nonexpo": "^1.0.11",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-startup-time": "^1.3.0",
    "react-native-tab-view": "^2.15.2",
    "react-native-vector-icons": "^7.1.0",
    "react-native-version-check": "^3.4.2",
    "react-native-video": "4.4.5",
    "react-native-webview": "^10.7.0",
    "react-native-youtube-iframe": "^1.2.4",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-filesystem-storage": "^3.0.0",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.12.0",
    "use-debounce": "^5.2.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@brainly/onesky-utils": "^1.4.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^26.0.19",
    "@types/react-native": "^0.63.2",
    "@types/react-native-material-menu": "^1.0.3",
    "@types/react-redux": "^7.1.15",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-native-codegen": "^0.0.6",
    "react-native-inhibit-warnings": "^1.0.1",
    "react-test-renderer": "17.0.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/__mock__/react-native-bootsplash.ts"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/*.{js,jsx,ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-test-renderer|react-native-startup-time|react-native-bootsplash)/)"
    ]
  },
  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  }
}
