{
  "name": "voke",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'iPhone 11'",
    "ios:os13": "react-native run-ios --simulator='iPhone 11 Pro iOS 13'",
    "ios:iphone8": "react-native run-ios --simulator='iPhone 8'",
    "ios8:iphone8os13": "react-native run-ios --simulator='iPhone 8 Pro iOS 13'",
    "ios:ipad": "react-native run-ios --simulator='iPad'",
    "ios:release": "react-native run-ios --configuration=release",
    "ios:fiximagebug": "npx react-native-fix-image",
    "start": "react-native start",
    "start:clean": "react-native start --reset-cache",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "detox:build:ios": "detox build --configuration ios",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "detox:test:ios": "clear && detox test --configuration ios",
    "detox:test:singlefile": "clear && detox test --configuration ios -f /e2e/adventureGroupDaily.e2e.js --debug-synchronization 400000",
    "detox:test:reuse": "clear && detox test  --reuse --configuration ios",
    "detox:test:withlogs": "detox test --loglevel verbose --configuration ios",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier ./src --ext .js,.jsx,.ts,.tsx",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run prettier && npm run lint",
    "pi": "cd ios/ && pod install",
    "open:ios": "open ios/Voke.xcworkspace/",
    "open": "npm run open:ios",
    "o": "npm run open:ios",
    "clean": "react-native-clean-project",
    "ios:beta": "bundle exec fastlane ios beta",
    "android:beta": "bundle exec fastlane android beta",
    "onesky:upload": "babel-node ./bin/oneskyUpload.js",
    "onesky:download": "babel-node ./bin/oneskyDownload.js"
  },
