{
  "name": "voke",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "sc": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "android": "adb devices && react-native run-android",
    "android:install:dev": "adb install android/app/build/outputs/apk/app-debug.apk",
    "android:install:release": "adb install android/app/build/outputs/apk/app-release.apk",
    "android:build:remove:system": "npm run android:build",
    "android:build": "react-native run-android --variant=release",
    "open": "open ios/Voke.xcodeproj/",
    "o": "npm run open",
    "reset-cache-watchman": "watchman watch-del-all && rm -rf node_modules && npm install && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "resolutions": {
    "react-deep-force-update": "1.1.2"
  },
