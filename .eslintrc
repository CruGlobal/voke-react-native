{
  "extends": [
    "react-native-wcandillon",
    "prettier",
    "prettier/react",
    "prettier/@typescript-eslint"
  ],
  "globals": {
    "LOG": true,
    "WARN": true,
    "__DEV__": true
  },
  "rules": {
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ],
    "camelcase": [
      "error",
      {
        "allow": [
          "active_messengers",
          "error_description",
          "first_name",
          "message_id",
          "messenger_id",
          "messenger_answer",
          "messenger_journeys",
          "messenger_journey_id",
          "messenger_journey_reports",
          "messenger_journey_steps",
          "messenger_journey_step_id",
          "step_kind",
          "screen_name",
          "screen_class",
          "vokebot_action",
          "organization_journeys",
          "page_number",
          "gating_period",
          "total_pages",
          "total_count",
          "language_code",
          "last_name",
          "limit_value"
        ]
      }
    ]
  }
}
